{
  "name": "eslint-config-pob",
  "version": "25.0.0",
  "description": "eslint config files",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/eslint-config-pob.git",
  "homepage": "https://github.com/christophehurpeau/eslint-config-pob",
  "engines": {
    "node": ">=6.5.0"
  },
  "main": "./lib/index.js",
  "scripts": {
    "lint": "eslint --ext .js,.jsx --fix rules/ plugins/ *.js",
    "preversion": "yarn run lint && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "post-checkout": "yarnhook",
      "post-merge": "yarnhook",
      "post-rewrite": "yarnhook",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{package.json,.eslintrc.json}": [
      "prettier --parser json --write",
      "git add"
    ],
    "lib/**/*.json": ["prettier --parser json --write", "git add"],
    "lib/**/*.js": ["eslint --fix --quiet", "git add"]
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-prefer-class-properties": "^1.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-unicorn": "^7.0.0"
  },
  "peerDependencies": {
    "eslint": "^5.9.0",
    "prettier": "^1.15.2"
  },
  "devDependencies": {
    "@commitlint/cli": "7.3.2",
    "@commitlint/config-conventional": "7.3.1",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-pob": "25.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-react": "7.12.3",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "pob-release": "4.6.3",
    "prettier": "1.16.0",
    "yarnhook": "0.3.0"
  }
}
