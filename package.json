{
  "private": true,
  "version": "29.2.0",
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "https://github.com:christophehurpeau/eslint-config-pob.git",
  "homepage": "https://github.com/christophehurpeau/eslint-config-pob",
  "engines": {
    "node": ">=6.5.0"
  },
  "workspaces": ["@pob/*"],
  "scripts": {
    "lint": "eslint .",
    "preversion": "eslint .",
    "release": "GH_TOKEN=$POB_GITHUB_TOKEN lerna version --conventional-commits --conventional-graduate --create-release=github -m 'chore: release' && lerna publish from-git"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "devDependencies": {
    "@pob/repo-config": "1.1.3",
    "@pob/version": "5.1.0",
    "@yarnpkg/pnpify": "2.0.0-rc.17",
    "eslint": "6.8.0",
    "eslint-import-resolver-node": "0.3.3",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-prefer-class-properties": "1.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-unicorn": "15.0.1",
    "husky": "4.2.1",
    "lerna": "3.20.2",
    "prettier": "1.19.1"
  }
}
